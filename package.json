{
  "type": "module",
  "name": "tailwind-plugin-device",
  "description": "Tailwind plugin to detect device input type",
  "version": "0.1.0",
  "author": "Nick K.",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./lib/device.js",
      "types": "./lib/device.d.ts"
    },
    "./lib/variants.js": null,
    "./lib/variants": null,
    "./lib/entries.js": null,
